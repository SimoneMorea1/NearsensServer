
@{
    ViewBag.Title = "Map";
}



<h2>Map</h2>

<div id="map-canvas" style="height: 50px; margin: 0; padding: 0; border:double 5px"></div>

<script type="text/javascript"
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA5X4IQJPxcWDRzKc-Ufq029N55AhJs0As">
</script>

<script>
    function initialize() {
        var mapOptions = {
            center: { lat: -34.397, lng: 150.644 },
            zoom: 8
        };
        var map = new google.maps.Map(document.getElementById('map-canvas'),
            mapOptions);
    }

    google.maps.event.addDomListener(window, 'load', initialize);
</script>

